new file mode 100644
--- /dev/null	(date 1698917340144)
+++ b/lib/views/speedtest.dart	(date 1698917340144)
@@ -0,0 +1,38 @@
+import 'package:flutter/material.dart';
+import 'package:webview_all/webview_all.dart';
+import 'package:webview_universal/webview_universal.dart';
+
+class SpeedTestView extends StatefulWidget {
+  const SpeedTestView({super.key});
+
+  @override
+  State createState() => _SpeedTestView();
+}
+
+class _SpeedTestView extends State<SpeedTestView> {
+  WebViewController webViewController = WebViewController();
+
+  @override
+  void initState() {
+    super.initState();
+    task();
+  }
+
+  Future<void> task() async {
+    await webViewController.init(
+      context: context,
+      uri: Uri.parse("https://www.metercustom.net/plugin/?th=w"), setState: (void Function() fn) {  },
+    );
+  }
+
+  @override
+  Widget build(BuildContext context) {
+    // todo: open this in a separate windows window
+    return SizedBox(
+      width: 400,
+      height: 400,
+      // child: Webview(url: "https://flutter.dev"),
+      child: WebView(controller: webViewController),
+    );
+  }
+}
